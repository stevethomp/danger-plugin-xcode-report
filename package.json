{
  "name": "danger-plugin-xcode-report",
  "description": "Add your Xcode test results, warnings and errors to Danger.",
  "author": {
    "name": "Steven Thompson",
    "email": "steve.thomp@me.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevethomp/danger-plugin-xcode-report.git"
  },
  "bugs": {
    "url": "https://github.com/stevethomp/danger-plugin-xcode-report/issues"
  },
  "homepage": "https://github.com/stevethomp/danger-plugin-xcode-report#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "xcode",
    "ios",
    "xctest"
  ],
  "version": "0.1.1",
  "main": "dist/index",
  "typings": "dist/index",
  "types": "types/index.d.ts",
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "build": "tsc",
    "test": "jest",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --parser typescript --no-semi --trailing-comma es5 --write --print-width 120",
    "prettier-project": "npm run prettier-write -- 'src/**/*.{ts,tsx}'",
    "lint": "tslint \"src/**/*.ts\""
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "@types/node": "^8.10.20",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "danger": "*",
    "danger-plugin-jira-issue": "^1.3.0",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "jest-cli": "^23.1.0",
    "lint-staged": "^3.4.1",
    "npm": "^6.0.0",
    "prettier": "^1.3.1",
    "semantic-release": "^6.3.6",
    "ts-jest": "^20.0.0",
    "tslint": "^5.4.3",
    "typescript": "^2.8.3",
    "validate-commit-msg": "^2.12.1"
  },
  "optionalDependencies": {
    "esdoc": "^0.5.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "npm run prettier-write --",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {}
}
